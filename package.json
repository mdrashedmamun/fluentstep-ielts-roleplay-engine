{
  "name": "fluentstep:-ielts-roleplay-engine",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "npm run validate:critical && npm run validate && tsx scripts/detectDataCorruption.ts",
    "build": "vite build",
    "preview": "vite preview",
    "validate": "tsx scripts/validateScenarios.ts",
    "validate:critical": "tsx scripts/validateCritical.ts",
    "validate:perfect": "tsx scripts/validatePerfectScenarios.ts",
    "validate:feedback": "tsx scripts/validateChunkFeedback.ts",
    "validate:alternatives": "tsx scripts/validateAnswerAlternatives.ts",
    "pre-commit": "npm run validate:critical",
    "audit": "tsx cli/auditLanguage.ts",
    "audit:report": "tsx cli/auditLanguage.ts --report-only",
    "audit:dry-run": "tsx cli/auditLanguage.ts --dry-run",
    "audit:test": "tsx scripts/testAuditSystem.ts",
    "audit:parallel": "tsx cli/auditOrchestrator.ts",
    "audit:worker": "tsx cli/auditWorker.ts",
    "audit:phase1": "tsx cli/auditOrchestrator.ts --phase=1",
    "audit:phase1:dry": "tsx cli/auditOrchestrator.ts --phase=1 --dry-run",
    "audit:apply-fixes": "tsx cli/applyAuditFixes.ts",
    "audit:verify": "npm run build && npm run validate",
    "extract:headway": "tsx scripts/extractHeadwayScenarios.ts",
    "extract:headway:dry": "tsx scripts/extractHeadwayScenarios.ts --dry-run",
    "qa-check": "tsx cli/qaCheck.ts",
    "qa-check:single": "tsx cli/qaCheck.ts",
    "qa-check:strict": "tsx cli/qaCheck.ts --strict",
    "qa-test": "tsx scripts/testQAAgent.ts",
    "qa-test:comprehensive": "tsx scripts/testQAComprehensive.ts",
    "export:scenarios": "tsx scripts/exportScenariosToMarkdown.ts",
    "generate:pattern-summaries": "tsx scripts/exportGeneratedSummaries.ts",
    "validate:enrichments": "tsx scripts/validateEnrichments.ts",
    "import:enrichments": "tsx scripts/importEnrichedScenarios.ts",
    "import:package": "tsx scripts/importContentPackage.ts",
    "migrate:chunk-ids": "tsx scripts/migrateChunkIds.ts",
    "generate:package": "tsx scripts/generatePackage.ts",
    "review:package": "tsx scripts/reviewPackage.ts",
    "create:package": "tsx scripts/createPackage.ts",
    "create:scenario": "tsx scripts/createScenarioHelper.ts",
    "test:e2e": "python3 tests/e2e/orchestrator.py",
    "test:e2e:tier1": "python3 -m pytest tests/e2e/scenarios/tier1_with_feedback.py -v",
    "test:e2e:single": "python3 -m pytest tests/e2e/scenarios/tier1_with_feedback.py -k",
    "test:e2e:meta": "python3 -m pytest tests/e2e/meta_tests.py -v",
    "test:e2e:report": "open tests/reports/final_report.html",
    "lint": "eslint src/ components/ services/ cli/ scripts/ --ext .ts,.tsx",
    "lint:fix": "eslint src/ components/ services/ cli/ scripts/ --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "quality": "npm run type-check && npm run lint && npm run build",
    "test:pre-merge": "npm run build && npm run test:e2e:tier1 && npm run qa-test",
    "test:pre-commit": "npm run build && npm run validate:critical && npm run test:e2e:tier1",
    "stage:create": "tsx scripts/stagingCLIHelper.ts create",
    "stage:submit": "tsx scripts/stagingCLIHelper.ts submit",
    "stage:status": "tsx scripts/stagingCLIHelper.ts status",
    "stage:list-ready": "tsx scripts/stagingCLIHelper.ts list-ready",
    "stage:list-approved": "tsx scripts/stagingCLIHelper.ts list-approved",
    "stage:validate": "tsx scripts/stagingValidateContent.ts",
    "stage:import": "tsx scripts/stagingImportApproved.ts",
    "stage:import:dry-run": "tsx scripts/stagingImportApproved.ts --dry-run",
    "stage:unlock": "tsx scripts/stagingCLIHelper.ts unlock",
    "stage:help": "tsx scripts/stagingCLIHelper.ts help",
    "pipeline:watch": "tsx scripts/content-pipeline/cli.ts watch",
    "pipeline:status": "tsx scripts/content-pipeline/cli.ts status",
    "pipeline:help": "tsx scripts/content-pipeline/cli.ts help"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "@floating-ui/react": "^0.27.17",
    "@google/genai": "^1.39.0",
    "@types/canvas-confetti": "^1.9.0",
    "@types/react-router-dom": "^5.3.3",
    "canvas-confetti": "^1.9.4",
    "groq-sdk": "^0.37.0",
    "openai": "^4.52.0",
    "pdf-parse": "^2.4.5",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^22.19.10",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vercel/node": "^5.6.2",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.0",
    "chalk": "^5.3.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "pdfjs-dist": "^5.4.624",
    "playwright": "^1.58.2",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
